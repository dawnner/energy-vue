art1.setOption({\r\n          grid: {\r\n            left: '25%',\r\n            right: '15%',\r\n            bottom: '1%',\r\n            top: '5%',\r\n          },\r\n          tooltip: {\r\n            trigger: 'axis',\r\n            axisPointer: {\r\n              type: 'none',\r\n            },\r\n            formatter: function(params) {\r\n              return (\r\n                params[0].name + ' : ' + params[0].value\r\n              );\r\n            },\r\n          },\r\n          xAxis: {\r\n            show: false,\r\n            type: 'value',\r\n          },\r\n          yAxis: [{\r\n              type: 'category',\r\n              inverse: true,\r\n              axisLabel: {\r\n                show: true,\r\n                margin: 75,\n                align: 'left',\r\n                color: '#fff',\r\n              },\r\n              splitLine: {\r\n                show: false\r\n              },\r\n              axisTick: {\r\n                show: false\r\n              },\r\n              axisLine: {\r\n                show: false\r\n              },\r\n              data: ['三相不平衡', '电压合格率', '谐波含量', '线损异常', '发电量异常', '功率因数超限'],\r\n            },\r\n            {\r\n              type: 'category',\r\n              inverse: true,\r\n              axisTick: 'none',\r\n              axisLine: 'none',\r\n              show: true,\r\n              axisLabel: {\r\n                textStyle: {\r\n                  color: '#c0c3d2',\r\n                  fontSize: '11',\r\n                },\r\n                formatter: function(value) {\r\n                  return value;\r\n                },\r\n              },\r\n              data: [3000, 10000, 8000, 7000, 8000, 6000],\r\n            },\r\n          ],\r\n          series: [{\r\n              name: '',\r\n              type: 'bar',\r\n              zlevel: 1,\r\n              itemStyle: {\r\n                normal: {\r\n                  barBorderRadius: 30,\r\n                  color: (params) =>