\n                symbol: function(value, params) {\r\n                  return params.data.img;\r\n                },\r\n                symbolSize: [33, 31],\r\n                symbolOffset: [0, -10],\r\n                z: 999,\r\n                data: mapDate,\r\n              },\r\n              {\r\n                type: 'effectScatter',\r\n                coordinateSystem: 'geo',\r\n                z: 12,\r\n                symbolOffset: [0, -7],\r\n                symbolSize: [10, 7],\r\n                showEffectOn: 'render',\r\n                rippleEffect: {\r\n                  period: 2,\r\n                  scale: 5,\r\n                  brushType: 'fill'\r\n                },\r\n                hoverAnimation: true,\r\n                data: mapDate,\r\n                itemStyle: {\r\n                  normal: {\r\n                    show: true,\r\n                    color: function(params) {\r\n                      if (params.data.datas) {\r\n                        if (params.data.datas < 3000) {\r\n                          return '#139071';\r\n                        }\r\n                        if ((params.data.datas) >= 4000 && (params.data.datas <= 5000)) {\r\n                          return '#BB963F';\r\n                        }\r\n                        if ((params.data.datas) >= 3000 && (params.data.datas <= 4000)) {\r\n                          return '#33ABC9';\r\n                        }\r\n                        if (params.data.datas > 5000) {\r\n                          return '#d04132';\r\n                        }\r\n                      }\r\n                    },\r\n                  },\r\n                },\r\n              },\r\n            ],\r\n          };\r\n          this.mchart1.setOption(option);\r\n          // });\r\n        };\r\n        window.addEventListener(\"resize\", () => {\r\n          this.mchart1.resize();\r\n        });\r\n      }\r\n    }\r\n  }\r\n</script>\n"]}]}